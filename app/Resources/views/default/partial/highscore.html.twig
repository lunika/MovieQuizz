<table class="table table-striped">
    <thead>
        <tr>
            <th>Name</th>
            <th>Score</th>
            <th>Duration</th>
        </tr>
    </thead>
    <tbody>
    {% for score in scores %}
        <tr>
            <td>{{ score.name }}</td>
            <td>{{ score.score }}</td>
            <td>{{ attribute(score, "getFormattedDuration") }}</td>
        </tr>
    {% endfor %}
    </tbody>
</table>